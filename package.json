{
  "name": "soia-language",
  "displayName": "Soia - Language Support",
  "description": "Language support for Soia - a language for representing data types, constants and RPC interfaces",
  "version": "0.1.7",
  "publisher": "TylerFibonacci",
  "engines": {
    "vscode": "^1.60.0"
  },
  "icon": "resources/soia-icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/gepheum/soia"
  },
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "soia",
    "protocol buffer",
    "protobuf",
    "serialization",
    "rpc"
  ],
  "activationEvents": [
    "onLanguage:soia"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "soia",
        "aliases": [
          "Soia",
          "soia"
        ],
        "extensions": [
          ".soia"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "soia",
        "scopeName": "source.soia",
        "path": "./syntaxes/soia.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "package": "vsce package"
  },
  "dependencies": {
    "soiac": "^1.0.44",
    "yaml": "^2.5.1",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.13.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "typescript": "^4.7.2",
    "vsce": "^1.97.0"
  }
}
